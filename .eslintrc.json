{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "plugin:import/typescript"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "@typescript-eslint"
    ],
    "rules": {
        "react/jsx-filename-extension": [0, { "extensions": [".js", ".jsx", ".ts", ".tsx"] }],
        "import/extensions": [0, "never"],
        "import/prefer-default-export": [0],
        "no-multiple-empty-lines": [2, {"max": 1, "maxEOF": 2}]
    },
    "settings": {
      "import/resolver": {
        "typescript": {}
      }
    }
}
